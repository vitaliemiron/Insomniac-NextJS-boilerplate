{
  "name": "insomniac",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "start:dev": "cross-env TS_NODE_PROJECT=tsconfig.server.json node --max_old_space_size=4024 -r ts-node/register server/index.ts",
    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "eslint './{src,pages}/**/*' --ext .tsx,.ts",
    "format": "npm run lint -- --fix",
    "pretty": "prettier --write --config .prettierrc.js {src,pages}/**/*.{ts,tsx}",
    "generate": "graphql-codegen --config codegen.yml",
    "analyze": "cross-env ANALYZE=true npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.1.4",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/introspection": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@next/bundle-analyzer": "^9.5.3",
    "@storybook/addon-actions": "6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "6.0.21",
    "@storybook/addons": "6.0.21",
    "@storybook/react": "6.0.21",
    "@types/express": "^4.17.8",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "prettier": "^2.1.1",
    "storybook": "^6.0.21",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
