{
  "name": "insomniac",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "dev": "npm run generate && cross-env TS_NODE_PROJECT=tsconfig.server.json node --max_old_space_size=4024 -r ts-node/register server/index.ts",
    "build": "npm run generate && npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "eslint './{src,pages}/**/*' --ext .tsx,.ts",
    "format": "npm run lint -- --fix",
    "pretty": "prettier --write --config .prettierrc.js {src,pages}/**/*.{ts,tsx}",
    "generate": "graphql-codegen --config codegen.yml",
    "analyze": "cross-env ANALYZE=true npm run build",
    "storybook": "start-storybook -s ./public -p 6006",
    "test": "jest --coverage",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "lodash": "^4.17.21",
    "next": "11.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/introspection": "^1.17.8",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@graphql-codegen/typescript-react-apollo": "^2.2.5",
    "@next/bundle-analyzer": "^10.2.3",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-docs": "^6.1.16",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "6.2.9",
    "@storybook/addon-storyshots": "^6.1.16",
    "@storybook/addon-storysource": "^6.1.16",
    "@storybook/addon-viewport": "^6.1.16",
    "@storybook/addons": "6.2.9",
    "@storybook/react": "6.2.9",
    "@storybook/source-loader": "^6.1.16",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@types/express": "^4.17.12",
    "@types/node": "^14.6.4",
    "@types/react": "^17.0.9",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-react-app": "^10.0.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "prettier": "^2.3.0",
    "storybook": "^6.0.21",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.3.2"
  }
}
